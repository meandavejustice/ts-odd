{
  "name": "@oddjs/odd",
  "version": "next",
  "description": "ODD SDK",
  "keywords": [
    "WebCrypto",
    "auth",
    "files",
    "distributed",
    "DAG",
    "DID",
    "IPFS",
    "IPLD",
    "UCAN",
    "WNFS"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js"
    ],
    "./*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js",
      "./*"
    ]
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oddsdk/ts-odd"
  },
  "homepage": "https://odd.dev",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "lint": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "prebuild": "rimraf lib dist && node scripts/gen-version.js",
    "build": "tsc && npm run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "start": "tsc -w",
    "test": "mocha",
    "test:gh-action": "TEST_ENV=gh-action npm run test",
    "test:imports": "madge src --ts-config tsconfig.json --extensions ts --circular --warning",
    "test:prod": "npm run build && npm run lint && npm run test:imports && npm run test:gh-action",
    "test:types": "cp -RT tests/types/ lib/ && npm run tsd",
    "test:unit": "mocha --watch --testPathPattern=src/",
    "test:wasm": "WNFS_WASM=true mocha",
    "prepare": "npm run build && npm run docs",
    "publish-dry": "npm publish --dry-run",
    "publish-alpha": "npm publish --tag alpha",
    "publish-latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^11.0.1",
    "@ipld/dag-cbor": "^9.0.0",
    "@ipld/dag-pb": "^4.0.2",
    "@libp2p/mplex": "^7.1.2",
    "@libp2p/peer-id": "^2.0.2",
    "@libp2p/websockets": "^5.0.4",
    "@multiformats/multiaddr": "^11.6.0",
    "blockstore-core": "^3.0.0",
    "blockstore-datastore-adapter": "^5.0.0",
    "datastore-core": "^8.0.4",
    "datastore-level": "^9.0.4",
    "debounce-promise": "^3.1.2",
    "events": "^3.3.0",
    "fission-bloom-filters": "1.7.1",
    "interface-blockstore": "^4.0.1",
    "ipfs-bitswap": "^16.0.0",
    "ipfs-unixfs-exporter": "^12.0.0",
    "ipfs-unixfs-importer": "^14.0.1",
    "it-all": "^2.0.1",
    "keystore-idb": "^0.15.5",
    "libp2p": "^0.42.2",
    "localforage": "^1.10.0",
    "multiformats": "^11.0.1",
    "one-webcrypto": "^1.0.3",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^4.0.3",
    "wnfs": "0.1.19"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@ipld/car": "^5.1.0",
    "@types/debounce-promise": "^3.1.6",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "expect": "^27.0.0",
    "fast-check": "^3.7.0",
    "globby": "^13.1.3",
    "madge": "^6.0.0",
    "mocha": "^10.2.0",
    "rimraf": "^4.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.26",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5",
    "util": "^0.12.4"
  }
}
