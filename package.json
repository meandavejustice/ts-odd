{
  "name": "webnative",
  "version": "0.36.3",
  "description": "Webnative SDK",
  "keywords": [
    "WebCrypto",
    "auth",
    "files",
    "distributed",
    "DAG",
    "DID",
    "IPFS",
    "IPLD",
    "UCAN",
    "WNFS"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js"
    ],
    "./*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js",
      "./*"
    ]
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fission-codes/webnative"
  },
  "homepage": "https://webnative.dev",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "lint": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "prebuild": "rimraf lib dist && node scripts/gen-version.js",
    "build": "tsc && npm run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "start": "tsc -w",
    "test": "mocha",
    "test:gh-action": "TEST_ENV=gh-action npm run test",
    "test:imports": "madge src --ts-config tsconfig.json --extensions ts --circular --warning",
    "test:prod": "npm run build && npm run lint && npm run test:imports && npm run test:gh-action",
    "test:types": "cp -RT tests/types/ lib/ && npm run tsd",
    "test:unit": "mocha --watch --testPathPattern=src/",
    "test:wasm": "WNFS_WASM=true mocha",
    "prepare": "npm run build && npm run docs",
    "publish-dry": "npm publish --dry-run",
    "publish-alpha": "npm publish --tag alpha",
    "publish-latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^8.0.0",
    "@ipld/dag-pb": "^3.0.1",
    "@libp2p/interface-keys": "^1.0.4",
    "@libp2p/peer-id": "^1.1.17",
    "@multiformats/multiaddr": "^11.1.0",
    "blockstore-core": "^2.0.2",
    "blockstore-datastore-adapter": "^4.0.0",
    "datastore-core": "^8.0.2",
    "datastore-level": "^9.0.4",
    "events": "^3.3.0",
    "fission-bloom-filters": "1.7.1",
    "ipfs-core-types": "0.13.0",
    "ipfs-repo": "^16.0.0",
    "keystore-idb": "^0.15.5",
    "localforage": "^1.10.0",
    "multiformats": "^10.0.2",
    "one-webcrypto": "^1.0.3",
    "throttle-debounce": "^3.0.1",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.0.0",
    "wnfs": "0.1.17"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@ipld/car": "^5.0.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.15.13",
    "eslint": "^8.7.0",
    "expect": "^27.4.6",
    "fast-check": "^3.3.0",
    "globby": "^13.1.2",
    "ipfs-core": "0.17.0",
    "localforage-driver-memory": "^1.0.5",
    "madge": "^5.0.1",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.8.4",
    "util": "^0.12.4"
  }
}
